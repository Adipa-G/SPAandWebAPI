  <appSettings>
    <add key="DatabaseFileName" value="Owin.db"/>
    <add key="serilog:minimum-level" value="Information"/>
    <add key="serilog:using:File" value="Serilog.Sinks.File"/>
    <add key="serilog:write-to:File.path" value="%LOGDIR%/log.txt"/>
    <add key="serilog:write-to:File.rollingInterval" value="Day"/>
    <add key="serilog:write-to:File.outputTemplate" value="{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] [{SourceContext}] {Message}{NewLine}{Exception}"/>
    <add key="serilog:write-to:File.retainedFileCountLimit" value="10"/>
  </appSettings>
